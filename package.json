{
  "name": "newrelic-kepler-agent",
  "version": "0.8.1",
  "description": "A New Relic Agent for Kepler",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc",
    "build:watch": "tsc -w",
    "clean": "rimraf ./dist",
    "dev": "npm run build && npm run build:watch",
    "prepack": "npm run build",
    "test": "jest --verbose true",
    "test:watch": "jest --watch",
    "test:coverage": "jest --collect-coverage",
    "lint": "eslint ./**/*.js",
    "lint:fix": "eslint ./**/*.js --fix",
    "docs:generate": "jsdoc -c jsdoc.json",
    "commit": "cz",
    "build:plugin": "rimraf plugin/build && tsc --build plugin"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "CHANGELOG.md",
    "THIRD_PARTY_NOTICES.md"
  ],
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(@?react-native|react-native-promise-rejection-utils)/)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "keywords": [
    "new relic",
    "kepler"
  ],
  "author": "New Relic",
  "license": "Apache-2.0",
  "homepage": "https://github.com/newrelic-experimental/newrelic-kepler-agent",
  "dependencies": {
    "@amazon-devices/react-native-device-info": "^2.0.0",
    "@amazon-devices/react-native-kepler": "~2.0.0",
    "@expo/config-plugins": "^7.2.2",
    "pako": "^2.1.0",
    "react-native-promise-rejection-utils": "0.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^18.11.18",
    "@types/pako": "^2.0.3",
    "@types/react": "^19.2.0",
    "ansi-regex": ">=5.0.1",
    "aws-sdk": "^2.1048.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^29.7.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "docdash": "^1.2.0",
    "edit-json-file": "^1.7.0",
    "eslint": "^8.29.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.2",
    "markdown-it": ">=12.3.2",
    "marked": ">=4.0.10",
    "node-notifier": ">=8.0.1",
    "shelljs": "^0.8.4",
    "ts-jest": "^29.1.2",
    "typescript": "^4.9.5",
    "unzipper": "^0.10.11"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "resolutions": {
    "@types/react": "^19.2.0"
  },
  "overrides": {}
}